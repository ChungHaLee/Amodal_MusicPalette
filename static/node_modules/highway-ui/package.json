{
  "name": "highway-ui",
  "version": "1.2.3",
  "main": "es/index.ts",
  "scripts": {
    "start": "npx dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "build": "father-build",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "prepublishOnly": "npm run build",
    "release": "npm publish"
  },
  "module": "es/index.js",
  "typings": "es/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "anim-event": "^1.0.17",
    "antd": "^4.19.2",
    "classnames": "^2.3.1",
    "cssprefix": "^2.0.17",
    "lodash": "^4.17.21",
    "m-class-list": "^1.1.10",
    "plain-draggable": "^2.5.14",
    "pointer-event": "^1.0.2",
    "prop-types": "^15.8.1",
    "react": "^16.12.0 || ^17.0.0",
    "react-color": "^2.19.3",
    "subjx": "0.3.6"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^12.1.2",
    "@types/classnames": "^2.3.1",
    "@types/jest": "^27.0.3",
    "@types/lodash": "^4.14.179",
    "@types/react": "^18.0.7",
    "@types/react-color": "^3.0.6",
    "@umijs/fabric": "^2.8.1",
    "@umijs/test": "^3.0.5",
    "babel-plugin-import": "^1.13.5",
    "dumi": "^1.1.0",
    "father-build": "^1.17.2",
    "gh-pages": "^3.0.0",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.1",
    "yorkie": "^2.0.0"
  },
  "peerDependencies": {
    "antd": "^4.19.2",
    "react": "^16.12.0 || ^17.0.0"
  }
}
